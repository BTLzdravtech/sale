<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.pos_ux</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="point_of_sale.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <setting id="pos_default_journals" position="after">
                <setting id="pos_block_invoice_download_setting" string="Block invoice donwload">
                    <field name="pos_block_invoice_download"/>
                </setting>
                <setting id="pos_billing_behavior_setting">
                   <field name="pos_billing_behavior"/>
                </setting>
            </setting>
        </field>
    </record>
</odoo>
